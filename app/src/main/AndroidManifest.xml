<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smartherd.helloworld2">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/van2"
        android:label="Hello World"
        android:roundIcon="@drawable/van2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddCom" />
        <activity
            android:name=".City"
            android:label="@string/title_activity_amsterdam"
            android:parentActivityName=".CitySearch" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginPage" />
        <activity
            android:name=".CitySearch"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".CommentSection"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />

        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
    </application>

</manifest>